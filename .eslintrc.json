{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-unsafe-optional-chaining": "off"
  }
}