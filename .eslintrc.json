{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jest/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-unsafe-optional-chaining": "off"
  }
}